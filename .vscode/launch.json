{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "cwd": "${fileDirname}",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": [
                "--pacman=AlphaBetaAgent",
                "--layout=smallClassic",
                "--agentArgs=depth=2",
            ],
        }
    ]
}